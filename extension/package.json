{
  "name": "vscode-color-json-picker",
  "displayName": "vscodeColorJsonPicker",
  "version": "0.0.2",
  "description": "pick colors from json file",
  "repository": {
    "type": "git",
    "url": "https://github.com/tangdaoyuan/vscode-color-json-picker",
    "directory": "extension"
  },
  "engines": {
    "vscode": "^1.66.0"
  },
  "publisher": "Tedy",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:author",
    "onCommand:cc.default",
    "onCommand:cc.all",
    "onCommand:cc.alpha",
    "onCommand:cc.color"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "author",
        "title": "Author"
      },
      {
        "command": "cc.default",
        "title": "Color JSON Picker Default"
      },
      {
        "command": "cc.alpha",
        "title": "Color JSON Picker With Alpha"
      },
      {
        "command": "cc.color",
        "title": "Color JSON Picker With Only Color"
      },
      {
        "command": "cc.all",
        "title": "Color JSON Picker With Only Color and Alpha"
      }
    ]
  }
}
