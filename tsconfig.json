{
  "exclude": [
    "**/__test__/*",
    "**/*.test.ts"
  ],
  "include": ["extension", "src"],
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": [
      "ESNext"
    ],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "skipDefaultLibCheck": true,
    "types": [
      "node"
    ]
  }
}
